<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job instance="Ziffity\InstagramFeed\Cron\DownloadInstImages" method="execute" name="download_insta_images">
            <config_path>zfinstagramfeed/display/instacron</config_path>
        </job>
        <job instance="Ziffity\InstagramFeed\Cron\RefreshInstaKey" method="execute" name="refresh_insta_key">
            <config_path>zfinstagramfeed/display/instarefreshcron</config_path>
        </job>
    </group>
</config>
